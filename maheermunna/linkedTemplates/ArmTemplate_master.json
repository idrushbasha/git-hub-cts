					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'maheermunna_ArmTemplate_0')]"
			]
		},
		{
			"name": "maheermunna_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
